<!DOCTYPE html>
<html lang="en">
  
<head>
    <%- include('./partials/head'); -%>
    
</head>
<body onload="startTime()">
    <%- include('./partials/header_sidebar'); -%>
    
    <!-- tap on top starts-->

        <!-- Page Sidebar Ends-->
        <div class="page-body">
          <div class="container-fluid">
            <div class="page-title">
              <div class="row">
                <div class="col-sm-6">
                  <h3><%=lan.General%> <%=lan.Setting%></h3>
                </div>
                <div class="col-sm-6">
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/index"><i data-feather="home"></i></a></li>
                    <li class="breadcrumb-item"><%=lan.Dashboard%></li>
                    <li class="breadcrumb-item active"><%=lan.General%> <%=lan.Setting%></li>
                  </ol>
                </div>
              </div>
            </div>
          </div>
          <!-- Container-fluid starts-->
          <div class="container-fluid">
            <div class="row">
              <div class="col-sm-12">
                <div class="card">
                  <div class="card-body">
                    <form  class="needs-validation" novalidate="" action="/settings/add_setting" method="post" enctype="multipart/form-data">
                      <div class="row">

                        <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6">
                        <div class="text-center form-group">
                            <% if(general.dark_image == "null") { %>
                                <img id="file-preview" width="150" height="150" src="../images/dashboard/default/profile.png" alt="<%= lan.image_alt %>">
                            <% } else { %>
                                <a href="../../<%=general.dark_image%>" target="_blank"><img id="file-preview" width="150" src="../../<%=general.dark_image%>" alt="<%= lan.image_alt %>"></a>
                            <% } %>
                        </div>
                        <div class="form-group">
                            <label class="col-form-label"><%=lan.Dark%> <%=lan.image %></label>
                            <input class="form-control" style="height: 47px;" type="file" accept="image/*" name="dark_image">
                        </div>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6">
                        <div class="text-center form-group">
                            <% if(general.light_image == "null") { %>
                                <img id="file-preview" width="150" height="150" src="../images/dashboard/default/profile.png" alt="<%= lan.image_alt %>">
                            <% } else { %>
                                <a href="../../<%=general.light_image%>" target="_blank"><img id="file-preview" width="150" src="../../<%=general.light_image%>" style="background-color: black;" alt="<%= lan.image_alt %>"></a>
                            <% } %>
                        </div>
                        <div class="form-group">
                            <label class="col-form-label"><%=lan.Light%> <%=lan.image %></label>
                            <input class="form-control" style="height: 47px;" type="file" accept="image/*" name="light_image">
                        </div>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Site%> <%=lan.Title%></label>
                        <input class="form-control" type="text" name="title" value="<%=general.title%>" placeholder="<%=lan.Enter%> <%=lan.Title%>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Alert%> <%=lan.Tone%></label>
                        <div class="row">
                            <div class="col-5">
                                <audio controls>
                                    <source src="../../<%=general.alert_tone%>" type="audio/mpeg">
                                </audio>
                            </div>
                            <div class="col-7" style="margin-top: 4px;">
                                <input class="form-control" type="file" accept="audio/*" name="alert_tone" style="height: 47px;">
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Currency%></label>
                        <input class="form-control" type="text" name="site_currency" value="<%=general.site_currency%>" placeholder="<%=lan.Enter%> <%=lan.Currency%>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label for="Thousands_Separator" class="form-label"><%=lan.Thousands_Separator%></label>
                        <select class="form-control wide" name="thousands_separator" required> 
                            <option value="1" <%= general.thousands_separator == 1 ? 'selected' : '' %> ><%=lan.Comma%></option>
                            <option value="2" <%= general.thousands_separator == 2 ? 'selected' : '' %> ><%=lan.Full_Stop%></option>
                            <option value="3" <%= general.thousands_separator == 3 ? 'selected' : '' %> ><%=lan.Apostrophe%></option>
                            <option value="4" <%= general.thousands_separator == 4 ? 'selected' : '' %> ><%=lan.None%></option>
                            <option value="5" <%= general.thousands_separator == 5 ? 'selected' : '' %> ><%=lan.Space%></option>
                        </select>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Google%> <%=lan.Map%> <%=lan.Key%></label>
                        <input class="form-control" type="text" name="google_map_key" value="*******************" placeholder="<%=lan.Enter%> <%=lan.Google%> <%=lan.Map%> <%=lan.Key%>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Ride%> <%=lan.Acceptance%> <%=lan.Radius%></label>
                        <input class="form-control" type="number" step="any" name="ride_radius" value="<%=general.ride_radius%>" placeholder="<%=lan.Enter%> <%=lan.Ride%> <%=lan.Acceptance%> <%=lan.Radius%>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                        <label class="col-form-label"><%=lan.Show%> <%=lan.Nearby%> <%=lan.Vehicle%> <%=lan.Radius%></label>
                        <input class="form-control" type="number" step="any" name="vehicle_radius" value="<%=general.vehicle_radius%>" placeholder="<%=lan.Enter%> <%=lan.Show%> <%=lan.Nearby%> <%=lan.Vehicle%> <%=lan.Radius%>" required>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%=lan.Driver%> <%=lan.Detail%>: <span class="f-w-500"><%=lan.Service%> (<%=lan.Module%>) <%=lan.Enable%>/<%=lan.Disable%></span></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6" style="margin-top: 30px;">
                      <div class="row">
                        <div class="col-sm-12 col-md-4 col-lg-4 col-xl-4">
                          <div class="d-flex mb-2">
                              <div class="icon-state switch-outline">
                                  <label class="switch">
                                      <input type="checkbox" name="outstation" <%= general.outstation == '1' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                                  </label>
                              </div>
                              <label class="col-form-label m-l-10"><%=lan.Outstation%></label>
                          </div>
                        </div>                      
                      
                        <div class="col-sm-12 col-md-4 col-lg-4 col-xl-4">
                            <div class="d-flex mb-2">
                                <div class="icon-state switch-outline">
                                    <label class="switch">
                                        <input type="checkbox" name="rental" <%= general.rental == '1' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                                    </label>
                                </div>
                                <label class="col-form-label m-l-10"><%=lan.Rental%></label>
                            </div>
                        </div>                      
                      
                        <div class="col-sm-12 col-md-4 col-lg-4 col-xl-4">
                            <div class="d-flex mb-2">
                                <div class="icon-state switch-outline">
                                    <label class="switch">
                                        <input type="checkbox" name="package" <%= general.package == '1' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                                    </label>
                                </div>
                                <label class="col-form-label m-l-10"><%=lan.Package%></label>
                            </div>
                        </div>     
                      </div>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <div class="row">
                        <div class="col-sm-12 col-md-4 col-lg-4 col-xl-4" style="margin-top: 30px;">
                          <div class="d-flex mb-2">
                            <div class="icon-state switch-outline">
                                <label class="switch">
                                    <input type="checkbox" name="def_driver" <%= general.def_driver == '1' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                                </label>
                            </div>
                            <label class="col-form-label m-l-10"><%=lan.Driver%> <%=lan.Approve%></label>
                          </div>
                        </div>
                      </div>
                    </div>   

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <div class="d-flex">
                        <label class="col-form-label"><%=lan.Wallet_Min_Balance_For_Withdraw%> (<label class="invosymbol"></label>)</label>
                      </div>
                      <input class="form-control" type="number" step="any" name="s_min_withdraw" value="<%=general.s_min_withdraw%>" placeholder="<%=lan.Enter%> <%=lan.Wallet_Min_Balance_For_Withdraw%>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%=lan.Select%> <%=lan.Default%> <%=lan.Payment%></label>
                      <select class="js-example-basic-single dricheck" name="dpayment" required>
                        <option value selected disabled><%=lan.Select%> <%=lan.Default%> <%=lan.Payment%></option>
                        <% payment_list.forEach((data) => { %>
                          <option value="<%= data.id %>" <%= general.default_payment == data.id ? 'selected' : '' %>><%= data.name %></option>
                        <% }) %>
                      </select>
                    </div>




                                            <hr class="mt-4">
                    <h5 class="pb-2"><%= lan.vehicle_ride_details %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.no_of_min %>. <%= lan.for_free_wait %></label>
                      <input class="form-control" type="number" step="any" name="driver_wait_time" value="<%=general.driver_wait_time%>" placeholder="<%=lan.enter_no_of_min_for_free_wait%>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.free_wait_time_after_per_minute_price %></label>
                      <input class="form-control" type="number" step="any" name="driver_wait_price" value="<%=general.driver_wait_price%>" placeholder="<%=lan.enter_free_wait_time_after_per_minute_price%>" required>
                    </div>
                    <div class="col-12">
                      <div class="row">
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.driver_offer_increment_counter %></label>
                          <input class="form-control" type="number" step="any" name="dri_offer_increment" value="<%=general.dri_offer_increment%>" placeholder="<%=lan.enter_driver_offer_increment_counter%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.offer_expired_time_driver %> (Driver)</label>
                          <input class="form-control" type="number" step="any" name="offer_time" value="<%=general.offer_time%>" placeholder="<%=lan.enter_offer_expired_time_driver%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.complete_offer_expired_time_customer %> (Customer)</label>
                          <input class="form-control" type="number" step="any" name="offer_expire_time" value="<%=general.offer_expire_time%>" placeholder="<%=lan.enter_complete_offer_expired_time_customer%>" required>
                        </div>
                      </div>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%= module[0].name %> <%= lan.ride_details %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.no_of_min %>. <%= lan.for_free_wait %></label>
                      <input class="form-control" type="number" step="any" name="out_no_min_free_wait" value="<%=general.out_no_min_free_wait%>" placeholder="<%=lan.enter_no_of_min_for_free_wait%>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.free_wait_time_after_per_minute_price %></label>
                      <input class="form-control" type="number" step="any" name="out_after_min_wait_price" value="<%=general.out_after_min_wait_price%>" placeholder="<%=lan.enter_free_wait_time_after_per_minute_price%>" required>
                    </div>
                    <div class="col-12">
                      <div class="row">
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.driver_offer_increment_counter %></label>
                          <input class="form-control" type="number" step="any" name="out_tot_offer_increment" value="<%=general.out_tot_offer_increment%>" placeholder="<%=lan.enter_driver_offer_increment_counter%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.offer_expired_time_driver %> (Driver)</label>
                          <input class="form-control" type="number" step="any" name="out_offer_exprie_time_cus" value="<%=general.out_offer_exprie_time_cus%>" placeholder="<%=lan.enter_offer_expired_time_driver%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.complete_offer_expired_time_customer %> (Customer)</label>
                          <input class="form-control" type="number" step="any" name="out_offer_exprie_time_dri" value="<%=general.out_offer_exprie_time_dri%>" placeholder="<%=lan.enter_complete_offer_expired_time_customer%>" required>
                        </div>
                      </div>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%= module[1].name %> <%= lan.ride_details %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.no_of_min %>. <%= lan.for_free_wait %></label>
                      <input class="form-control" type="number" step="any" name="ren_no_min_free_wait" value="<%=general.ren_no_min_free_wait%>" placeholder="<%=lan.enter_no_of_min_for_free_wait%>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.free_wait_time_after_per_minute_price %></label>
                      <input class="form-control" type="number" step="any" name="ren_after_min_wait_price" value="<%=general.ren_after_min_wait_price%>" placeholder="<%=lan.enter_free_wait_time_after_per_minute_price%>" required>
                    </div>
                    <div class="col-12">
                      <div class="row">
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.driver_offer_increment_counter %></label>
                          <input class="form-control" type="number" step="any" name="ren_tot_offer_increment" value="<%=general.ren_tot_offer_increment%>" placeholder="<%=lan.enter_driver_offer_increment_counter%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.offer_expired_time_driver %> (Driver)</label>
                          <input class="form-control" type="number" step="any" name="ren_offer_exprie_time_cus" value="<%=general.ren_offer_exprie_time_cus%>" placeholder="<%=lan.enter_offer_expired_time_driver%>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.complete_offer_expired_time_customer %> (Customer)</label>
                          <input class="form-control" type="number" step="any" name="ren_offer_exprie_time_dri" value="<%=general.ren_offer_exprie_time_dri%>" placeholder="<%=lan.enter_complete_offer_expired_time_customer%>" required>
                        </div>
                      </div>
                    </div>




                                            <hr class="mt-4">
                    <h5 class="pb-2"><%= module[2].name %> <%= lan.ride_details %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.no_of_min %>. <%= lan.for_free_wait %></label>
                      <input class="form-control" type="number" step="any" name="pack_no_min_free_wait" value="<%=general.pack_no_min_free_wait%>" placeholder="<%= lan.enter %> <%= lan.no_of_min %>. <%= lan.for_free_wait %>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.free_wait_time_after_per_minute_price %></label>
                      <input class="form-control" type="number" step="any" name="pack_after_min_wait_price" value="<%=general.pack_after_min_wait_price%>" placeholder="<%= lan.enter %> <%= lan.free_wait_time_after_per_minute_price %>" required>
                    </div>
                    <div class="col-12">
                      <div class="row">
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.driver %> <%= lan.offer %> <%= lan.increment %> <%= lan.counter %></label>
                          <input class="form-control" type="number" step="any" name="pack_tot_offer_increment" value="<%=general.pack_tot_offer_increment%>" placeholder="<%= lan.enter %> <%= lan.driver %> <%= lan.offer %> <%= lan.increment %> <%= lan.counter %>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.offer %> <%= lan.expired %> <%= lan.time_in_second %> (<%= lan.driver %>)</label>
                          <input class="form-control" type="number" step="any" name="pack_offer_exprie_time_cus" value="<%=general.pack_offer_exprie_time_cus%>" placeholder="<%= lan.enter %> <%= lan.offer %> <%= lan.expired %> <%= lan.time_in_second %>" required>
                        </div>
                        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                          <label class="col-form-label"><%= lan.complete %> <%= lan.offer %> <%= lan.expired %> <%= lan.time_in_second %> (<%= lan.customer %>)</label>
                          <input class="form-control" type="number" step="any" name="pack_offer_exprie_time_dri" value="<%=general.pack_offer_exprie_time_dri%>" placeholder="<%= lan.enter %> <%= lan.complete %> <%= lan.offer %> <%= lan.expired %> <%= lan.time_in_second %>" required>
                        </div>
                      </div>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%= lan.customer %> <%= lan.sign_up %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.signup %> <%= lan.credit %></label>
                      <input class="form-control" type="number" step="any" name="signup_credit" value="<%=general.signup_credit%>" placeholder="<%= lan.enter %> <%= lan.signup %> <%= lan.credit %>" required>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.refer %> <%= lan.credit %></label>
                      <input class="form-control" type="number" step="any" name="refer_credit" value="<%=general.refer_credit%>" placeholder="<%= lan.enter %> <%= lan.refer %> <%= lan.credit %>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <div class="row">
                        <label class="col-form-label"><%= lan.commission %> <%= lan.rate %></label>
                        <div class="col-sm-6 col-md-8 col-lg-8 col-xl-8">
                          <input class="form-control" type="number" step="any" name="commission_rate" value="<%=general.commission_rate%>" placeholder="<%= lan.enter %> <%= lan.commission %> <%= lan.rate %>" required>
                        </div>
                        <div class="col-sm-6 col-md-4 col-lg-4 col-xl-4 d-flex">
                          <div class="col-4">
                            <input class="form-control" type="text" placeholder="<%= lan.fix %>" readonly>
                          </div>
                          <div class="col-4 text-center">
                            <div class="icon-state switch-outline" style="padding-top: 6px;">
                              <label class="switch">
                                <input type="checkbox" name="commisiion_type" <%= general.commisiion_type == '%' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                              </label>
                            </div>
                          </div>
                          <div class="col-4">
                            <input class="form-control" type="text" placeholder="%" readonly>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <div class="row">
                        <label class="col-form-label"><%= lan.weather %> <%= lan.change %></label>
                        <div class="col-sm-6 col-md-8 col-lg-8 col-xl-8">
                          <input class="form-control" type="number" step="any" name="weather_price" value="<%=general.weather_price%>" placeholder="<%= lan.enter %> <%= lan.weather %> <%= lan.change %>" required>
                        </div>
                        <div class="col-sm-6 col-md-4 col-lg-4 col-xl-4 d-flex">
                          <div class="col-4">
                            <input class="form-control" type="text" placeholder="<%= lan.fix %>" readonly>
                          </div>
                          <div class="col-4 text-center">
                            <div class="icon-state switch-outline" style="padding-top: 6px;">
                              <label class="switch">
                                <input type="checkbox" name="weather_type" <%= general.weather_type == '%' ? 'checked' : '' %>><span class="switch-state bg-success"></span>
                              </label>
                            </div>
                          </div>
                          <div class="col-4">
                            <input class="form-control" type="text" placeholder="%" readonly>
                          </div>
                        </div>
                      </div>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%= lan.one %> <%= lan.signal %> <%= lan.detail %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.app %> <%= lan.id %></label>
                      <input class="form-control" type="text" name="one_app_id" value="******************" placeholder="<%= lan.enter %> <%= lan.app %> <%= lan.id %>" required>
                    </div>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label class="col-form-label"><%= lan.api %> <%= lan.key %></label>
                      <input class="form-control" type="text" name="one_api_key" value="***********************" placeholder="<%= lan.enter %> <%= lan.api %> <%= lan.key %>" required>
                    </div>

                    <hr class="mt-4">
                    <h5 class="pb-2"><%= lan.select %> <%= lan.sms %> <%= lan.type %></h5>

                    <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                      <label for="Thousands_Separator" class="form-label"><%= lan.sms %> <%= lan.type %></label>
                      <select class="form-control wide" name="smstype" required> 
                        <option value selected disabled><%= lan.select %> <%= lan.sms %> <%= lan.type %></option>
                        <option value="1" <%= general.sms_type == 1 ? 'selected' : '' %> ><%= lan.msg91 %></option>
                        <option value="2" <%= general.sms_type == 2 ? 'selected' : '' %> ><%= lan.twilio %></option>
                        <option value="4" <%= general.sms_type == 4 ? 'selected' : '' %> ><%= lan.no_auth %></option>
                      </select>
                    </div>

